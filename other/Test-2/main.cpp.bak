const double eps(1e-8);

struct node
{
      int num;
      node* next;
};

void Insert(node *& now,int x)
{
      now->next=new node;
}

void print(node *p)
{
      while(p->next!=NULL)
      {
            getchar();
            printf("%d ",p->num);
      }

}
int main()
{
      ios_base::sync_with_stdio(false);
      cin.tie(0);
      node *head,*now;
      head=now;
      now=new node;
      cout<<(head)<<endl;
      int x;
      while(cin>>x,x>=0)
      {
            cout<<"now="<<now<<endl;
            Insert(now,x);
      }
      cout<<(head)<<endl;
      print(head);
      return 0;
}
