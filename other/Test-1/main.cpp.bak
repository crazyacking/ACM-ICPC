#include<bits/stdc++.h>
using namespace std;
long long a[200005];
long long as[200005];
long long n,m,k;
long long main()
{
      scanf("%I64d %I64d %I64d",&n,&m,&k);
      long long p=1;
      long long i;
      for(i=0; i<n; i++)
      {
            scanf("%I64d",&a[i]);
      }
      for(i=0; i<m; i++) p*=k;
      for(i=0; i<n; i++) as[i]=a[i]*p;
      long long item=0;
      for(i=0; i<n; i++)
      {
            as[i]|=item;
            item|=a[i];
      }
      item=0;
      for(i=n-1; i>=0; i--)
      {
            as[i]=as[i]|item;
            item=item|a[i];
      }
      long long max_as=0;
      for(i=0; i<n; i++)
      {
            max_as=max(max_as,as[i]);
      }
      cout<<max_as<<endl;
      return 0;
}
