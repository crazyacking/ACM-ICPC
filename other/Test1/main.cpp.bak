#include<iostream>
#include<cstdio>
#include<cstring>
#include<cmath>

using namespace std;

int main()
{
      int t,n;
      scanf("%d",&t);
      while(t--)
      {
            scanf("%d",&n);
            double tmp=n*log10(double(n));  //这里得强制转换，否则Compilation Error
            double res=tmp-floor(tmp);
            printf("%d\n",(int)pow(10.0,res));
      }
      return 0;
}